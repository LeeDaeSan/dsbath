<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dsbath.admin.model.mapper.TileCodeMappingMapper">


<!--============== 타일 코드 맵핑 등록 START ==============-->
	<insert id="insert" parameterType="tileCodeMapping">
		INSERT INTO TileCodeMapping
		(
			BathDesignIdx,
			TileCodeIdx
		)
		VALUES
		(
			#{bathDesignIdx},
			#{tileCodeIdx}
		)
	</insert>
<!--============== 타일 코드 맵핑 등록 END ==============-->


</mapper>