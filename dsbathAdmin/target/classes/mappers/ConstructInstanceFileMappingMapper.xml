<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dsbath.admin.model.mapper.ConstructInstanceFileMappingMapper">

<!--============== 시공사례 공통파일 맵핑 등록 START ==============-->
	<insert id="insert" parameterType="constructInstanceFileMapping">
		INSERT INTO ConstructInstanceFileMapping
		(
			ConstructInstanceIdx,
			CommonFileIdx
		)
		VALUES
		(
			#{constructInstanceIdx},
			#{commonFileIdx}
		)
	</insert>
<!--============== 시공사례 공통파일 맵핑 등록 END ==============-->


<!--============== 시공사례 공통파일 맵핑 삭제 START ==============-->
	<delete id="delete" parameterType="int">
		DELETE FROM ConstructInstanceFileMapping
		 WHERE ConstructInstanceIdx = #{constructInstanceIdx}
	</delete>
<!--============== 시공사례 공통파일 맵핑 삭제 END ==============-->

</mapper>